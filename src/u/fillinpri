#!/bin/sh

export LC_ALL=C

dc=/tmp/$$.mwp.dict

sed '
	s/1, .},/P00.&/
	s/2, 1},/P01.&/
	s/2, 2},/P02.&/
	s/2, 0},/P03.&/
	s/3, 1},/P04.&/
	s/4, 1},/P05.&/
	s/0, 1},/P06.&/
	s/3, 2},/P07.&/
	s/4, 2},/P08.&/
	s/0, 2},/P09.&/
	s/3, 0},/P10.&/
	s/4, 0},/P11.&/
	s/0, 0},/P12.&/
' | sort -sk 4 | sed '
	s/\<P..[.]\(., .},\)/\1/
' >$dc

cnt=`wc -l $dc`

`dirname $0`/newr $cnt		\
| paste -d'@' - $dc	\
| sed 's/^\([0-9]*\)@\([^ ][^ ]*\) ?????/\2 \1/'
